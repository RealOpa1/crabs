{
  "manifest_version": 2,
  "name": "URLex",
  "version": "1.3",
  "permissions": [
    "tabs",
    "activeTab",
    "http://127.0.0.1:5000/log"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

